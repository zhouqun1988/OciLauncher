<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:baselineAligned="false"
    android:gravity="center"
    android:orientation="horizontal"
    android:weightSum="800" >

    <include
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="160"
        layout="@layout/launch_screen_weather_time" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="590"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/cell_margin"
            android:layout_marginRight="@dimen/cell_half_margin"
            android:layout_weight="1.0"
            android:baselineAligned="false"
            android:gravity="center"
            android:orientation="vertical" >

            <com.allwinner.theatreplayer.launcher.view.iLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginBottom="@dimen/cell_half_margin"
                android:layout_weight="1.0"
                android:background="#00000000"
                android:clickable="true"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    style="@style/cell_icon"
                    android:layout_marginLeft="@dimen/icon_margin_left"
                    android:contentDescription="@null" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/text_img_margin_bottom"
                    android:layout_marginLeft="@dimen/icon_margin_left"
                    android:textColor="@color/white"
                    android:textSize="@dimen/textsize_medium" />
            </com.allwinner.theatreplayer.launcher.view.iLinearLayout>

            <LinearLayout
                style="@style/cell_layout_1"
                android:layout_marginTop="@dimen/cell_half_margin"
                android:layout_weight="1.0"
                android:baselineAligned="false"
                android:orientation="horizontal" >

                <com.allwinner.theatreplayer.launcher.view.iLinearLayout
                    style="@style/cell_layout_2"
                    android:layout_marginRight="@dimen/cell_half_margin"
                    android:background="#00000000" >

                    <ImageView
                        style="@style/cell_icon"
                        android:contentDescription="@null" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/text_img_margin_bottom"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/textsize_medium" />
                </com.allwinner.theatreplayer.launcher.view.iLinearLayout>

                <com.allwinner.theatreplayer.launcher.view.iLinearLayout
                    style="@style/cell_layout_2"
                    android:layout_marginLeft="@dimen/cell_half_margin"
                    android:background="#00000000" >

                    <ImageView
                        style="@style/cell_icon"
                        android:contentDescription="@null" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/text_img_margin_bottom"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/textsize_medium" />
                </com.allwinner.theatreplayer.launcher.view.iLinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/cell_half_margin"
            android:layout_weight="1.0"
            android:baselineAligned="false"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginBottom="@dimen/cell_half_margin"
                android:layout_weight="1.0"
                android:baselineAligned="false"
                android:gravity="center"
                android:orientation="horizontal" >

                <com.allwinner.theatreplayer.launcher.view.iLinearLayout
                    style="@style/cell_layout_2"
                    android:layout_marginRight="@dimen/cell_half_margin"
                    android:background="#00000000" >

                    <ImageView
                        style="@style/cell_icon"
                        android:contentDescription="@null" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/text_img_margin_bottom"
                        android:gravity="center"
                        android:textColor="@color/white"
                        android:textSize="@dimen/textsize_medium" />
                </com.allwinner.theatreplayer.launcher.view.iLinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/cell_half_margin"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <com.allwinner.theatreplayer.launcher.view.iLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1.0"
                        android:background="#00000000"
                        android:gravity="center"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0"
                            android:contentDescription="@null" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0"
                            android:textColor="@color/white"
                            android:textSize="@dimen/textsize_medium" />
                    </com.allwinner.theatreplayer.launcher.view.iLinearLayout>

                    <com.allwinner.theatreplayer.launcher.view.iLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginTop="@dimen/cell_margin"
                        android:layout_weight="1.0"
                        android:background="#00000000"
                        android:gravity="center"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1.0"
                            android:contentDescription="@null" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1.0"
                            android:textColor="@color/white"
                            android:textSize="@dimen/textsize_medium" />
                    </com.allwinner.theatreplayer.launcher.view.iLinearLayout>
                </LinearLayout>
            </LinearLayout>

            <com.allwinner.theatreplayer.launcher.view.iLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/cell_half_margin"
                android:layout_weight="1.0"
                android:background="#00000000"
                android:clickable="true"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    style="@style/cell_icon"
                    android:layout_marginRight="@dimen/icon_margin_right"
                    android:contentDescription="@null" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/text_img_margin_bottom"
                    android:layout_marginRight="@dimen/icon_margin_right"
                    android:textColor="@color/white"
                    android:textSize="@dimen/textsize_medium" />
            </com.allwinner.theatreplayer.launcher.view.iLinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>